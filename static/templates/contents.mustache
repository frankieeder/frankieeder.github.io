{{#contents}}
    <div class="content">
        {{#rows}}
            {{#title}}
                <h2>{{.}}</h2>
            {{/title}}
            {{#subtitle}}
                <h4>{{.}}</h4>
            {{/subtitle}}
            {{#subsubtitle}}
                <h6>{{.}}</h6>
            {{/subsubtitle}}
            {{#type_photo_scrollbox}}
                <div
                        class="scrollbox {{#thumbnail}}thumbnails thumbsmall{{/thumbnail}}"
                        style="{{#thumbnail}}height: 250px;{{/thumbnail}} ">
                {{> photo_scrollbox}}
                </div>
            {{/type_photo_scrollbox}}
            {{#type_image}}
                {{> image}}
            {{/type_image}}
            {{#type_vimeo}}
                {{> vimeo_embed}}
            {{/type_vimeo}}
        {{/rows}}
    </div>
{{/contents}}